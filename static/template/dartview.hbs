<div class='score-card'>
	<h2>Game: {{options.name}}{{#if options.winner}}  Winner: {{options.winner}}{{/if}}</h2>
		<div class='throws'>Throws</div>
		<div class='name'>Name</div>
		<div class='score'>Score</div>
		<div class='score'>To Win</div>
		<div class='bust'>Busts</div>
		<div class='winner'>Winner</div>
	{{#each players}}
		<div class='throws'>{{#if throwsLeft}}{{throwsLeft}}{{else}}-{{/if}}</div>
		<div class='name'>{{name}}</div>
		<div class='score'>{{score}}</div>
		<div class='score'>{{scoreToWin}}</div>
		<div class='bust'>{{busts}}</div>
		<div class='winner'>{{#if won}}Winner{{else}}-{{/if}}</div>
	{{/each}}
</div>
<div class='throw-history'>
	<h2>History</h2>
		<div class='name'>Player</div>
		<div class='region'>Region</div>
		<div class='multi'>Multiplyer</div>
		<div class='total'>Total</div>
	{{#each dartsThrown}}
		<div class='name'>{{name}}</div>
		<div class='region'>{{region}}</div>
		<div class='multi'>{{multi}}</div>
		<div class='total'>{{total}}</div>
	{{/each}}
</div>